TO GORBE :M :S
  IF :S = 1 THEN FD :M ELSE GORBE :M/2 :S-1 LEFT 90 GORBE :M/2 :S-1 RIGHT 180 GORBE :M/2 :S-1 LEFT 90 GORBE :M/2 :S-1
END

TO ABRA :X :N
  IF :N > 0 THEN REPEAT 3 [ ABRA :X/2 :N-1 FD :X RT 120 ]
END

TO RAJZ :X :N
  DRAW RT 30 ABRA :X :N
END

TO ABRA2 :X :N
  REPEAT 3 [RT 120 FD :X ]
  IF :N > 0 THEN BACK :X/2 LT 60 ABRA2 :X/2 :N-1
END

TO RAJZ2 :X :N
  DRAW LEFT 30 ABRA2 :X :N
END

